<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>

<%= link_to "new", new_post_path %>

<table>
  <tr>
    <th>ID</th><th>title</th>
  </tr>
  <% @index.each do |i| %>
    <tr>
      <td>
        <%= i["id"] %>
      </td>
      <td>
        <%= i["title"] %>
      </td>
     
      <td>
        <%= link_to "show", post_url(i["id"]) %>
      </td>
      <td>
        <%= link_to "edit", edit_post_url(i["id"]) %>
      </td>
    </tr>
    <% end %>
</table>
